   "source": [
    "from collections import Counter\n",
    "#Cut the sticks solution\n",
    "def cutTheSticks(arr):\n",
    "    result = []\n",
    "    n = len(arr)\n",
    "    s = Counter(arr)\n",
    "\n",
    "    for i in sorted(s.keys()):\n",
    "        result.append(n)\n",
    "        n -= s[i]\n",
    "    return result"
   ]
  }
